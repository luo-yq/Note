yum install -y lrzsz
cd /usr/local
rz 上传文件到Linux

tar -xvf jdk-7u76-linux-x64.tar
rm -f jdk-7u76-linux-x64.tar

vi /etc/profile 

JAVA_HOME=/usr/local/jdk1.7.0_76
CLASS_PATH=.:$JAVA_HOME/lib/dt.jar:$JAVA_HOME/lib/tools.jar
PATH=$JAVA_HOME/bin:$PATH
export JAVA_HOME CLASS_PATH PATH

source /etc/profile