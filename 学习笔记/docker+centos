docker pull wurstmeister/zookeeper  
docker pull wurstmeister/kafka  


docker run -d --name zookeeper -p 2181 -t wurstmeister/zookeeper  
docker run --name kafka -e HOST_IP=localhost -e KAFKA_ADVERTISED_PORT=9092 -e KAFKA_BROKER_ID=1 -e ZK=zk -p 9092 --link zookeeper:zk -t wurstmeister/kafka  


docker exec -it ${CONTAINER ID} /bin/bash   
cd opt/kafka_2.11-0.10.1.1/   
bin/kafka-topics.sh --create --zookeeper zookeeper:2181 --replication-factor 1 --partitions 1 --topic mykafka  
bin/kafka-console-producer.sh --broker-list localhost:9092 --topic mykafka  
bin/kafka-console-consumer.sh --zookeeper zookeeper:2181 --topic mykafka --from-beginning  










docker pull tvial/docker-mailserver:latest

curl -o setup.sh https://raw.githubusercontent.com/tomav/docker-mailserver/master/setup.sh
chmod a+x ./setup.sh
curl -o docker-compose.yml https://raw.githubusercontent.com/tomav/docker-mailserver/master/docker-compose.yml.dist
curl -o .env https://raw.githubusercontent.com/tomav/docker-mailserver/master/.env.dist

#修改主机
vi .env 

./setup.sh email add admin@192.168.1.107 123456
./setup.sh config dkim

#Creating DKIM private key /tmp/docker-mailserver/opendkim/keys/192.168.1.107/mail.private
#Creating DKIM KeyTable
#Creating DKIM SigningTable
#Creating DKIM TrustedHosts
#Now the keys are generated, you can configure your DNS server by just pasting the content of config/opendkim/keys/192.168.1.107/mail.txt 

docker-compose up -d mail







